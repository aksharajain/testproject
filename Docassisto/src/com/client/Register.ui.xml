<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:h="urn:import:gwt.material.design.client.ui.html">
	<ui:style>
	.panel{
 background: url("http://2.bp.blogspot.com/-x4D3EPELYRU/VMHu52a4qUI/AAAAAAAAHek/EmnIgqTtu_U/s0/fon116%2B2160x1920.png");
 background-size: 40%;
 padding: 0px !important;
 padding-top: 5% !important;
 }
 .fieldPanel{
 background: white;
 padding: 6%;
 }
 .imgProfile{
 margin-top: -100px;
 width: 10%;
 margin-left: 5%;
 border: 5px solid #fff;
 }
 
 .login_demo{
 padding: 4%;
 }
 
	</ui:style>
	<g:HTMLPanel>
	 <m:MaterialPanel   shadow="1" addStyleNames="{style.panel}">
    <m:MaterialPanel addStyleNames="{style.fieldPanel}">
     <m:MaterialImage url="http://5.9.195.187/~aiimshis/images/doctor-icon.png" type="CIRCLE" addStyleNames="{style.imgProfile} z-depth-1"/>
	  <m:MaterialLabel text="Regsitration Form" fontSize="30px"  ></m:MaterialLabel>
	  <m:MaterialTextBox ui:field="t1" label="Clinic Name" width=" 100%"  iconType="ACCOUNT_CIRCLE" />
	  <m:MaterialTextBox ui:field="t7" label="Clinic Pass" width=" 100%"  iconType="ACCOUNT_CIRCLE" />  
	  <m:MaterialTextBox ui:field="t2" label="Your Name" width=" 100%" iconType="DATE_RANGE" />
	  <m:MaterialTextBox ui:field="t3" label="Doctor/Compounder" width=" 100%" iconType="DATE_RANGE" />
	  <m:MaterialTextBox ui:field="t4" label="Email" width=" 100%" iconType="PHONE" /> 
	  <m:MaterialTextBox ui:field="t5" label="Username" width=" 100%" iconType="WC" />
	  <m:MaterialTextBox ui:field="t6" label="Password"  width=" 100%" iconType="FACE" />
	  <m:MaterialButton ui:field="b1" />
	   </m:MaterialPanel>
   </m:MaterialPanel>
	</g:HTMLPanel>
</ui:UiBinder> 


